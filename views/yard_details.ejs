<body>
  <%- include('nav') %>
<div id="div" class="container m-5 yard_search">
  <h3 id="dncpop"><%= test.DNC %>  </h3>
  <h3 >Yard Name : <%= test.Yard_Name %> </h3>
  <h3>Address : <%= test.address %>, <%= test.city %>, <%= test.state %>, <%= test.ZIP %>.   </h3>
<h3 class="ejs">Name : <span class="mongo"><%= test.Contact %> </span></h3>
<h3>Phone : <%= test.Phone %>  </h3>
<h3>Fax : <%= test.FAX %> </h3>
<h3 class="ejs">Email : <span class="mongo"><%= test.Email %> </span></h3>
<h3 class="ejs">Warranty : <span class="mongo"><%= test.Warranty %></span> </h3>
<h3>Rating : <%= test.Rating %></h3>
<h3>No.of Reviews : <%= test.Reviews %></h3>
</div>
<a  class="btn-primary" href="/edit/<%=test._id%>">Update</a>
<a class="btn-danger" href="/DNC/<%=test._id%>">Edit DNC</a>

<script type="text/javascript" src="/js/index.js"></script>

</body>
