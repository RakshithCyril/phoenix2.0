<style>
  h3{
    color: whitesmoke;
  }
  #div{
    padding-left: 500px;
    padding-top: 100px;
  }
  .btn-danger{
    text-decoration: none;
     padding: 6px; 
     margin-top: -8px;
    margin-left: 10px;
     border-radius: 10px;
     outline: none;
     position: absolute;
  }
  .btn-primary{
    text-decoration: none; 
    padding: 8px; 
    margin-left: 800px; 
    border-radius: 10px;
    outline: none;
  }
  @import url('https://fonts.googleapis.com/css2?family=Raleway:wght@100&display=swap');
  
  .sticky {
  position: fixed;
  top: 0;
  width: 100%;
}
